{
  "AllowedHosts": "*",
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "NacosConfig": {
    //服务分组
    "GroupName": "DEFAULT_GROUP",
    // 服务注册到的命名空间id
    "Namespace": "public",
    "UserName": "nacos",
    "Password": "nacos",
    "Ip": "localhost",
    // 端口,需要和启动运行的端口一致
    "Port": 5011,
    // 权重
    "Weight": 100,
    // nacos服务端地址 172.26.94.136
    "ServerAddresses": [ "http://localhost:8848" ],
    // 服务名称
    "ServiceName": "yangxj96-serve-example-csharp",
    // 选择与前缀匹配的IP地址作为业务注册IP
    "PreferredNetworks": "192.168.2",
    "AccessKey": "",
    "SecretKey": "",
    "ClusterName": "DEFAULT",
    // 为true时，通过 gRPC 去和 nacos server 交互，nacos 2.x版本要设置为true
    "ConfigUseRpc": true,
    "DefaultTimeOut": 15000,
    "Ephemeral": true,
    "InstanceEnabled": true,
    //  WeightRandom WeightRoundRobin
    "LBStrategy": "WeightRandom",
    "ListenInterval": 1000,
    "NamingLoadCacheAtStart": "",
    // 为true时，通过 gRPC 去和 nacos server 交互, nacos 2.x版本要设置为true
    "NamingUseRpc": true,
    "RegisterEnabled": true,
    "Secure": false
    // "EndPoint": "",
  }
}
